//http://logging.apache.org/log4j/2.x/manual/layouts.html
{
    "configuration": {
        "name": "Default",
        "appenders": {
            "Console": {
                "name":"Console",
                "target":"SYSTEM_OUT",
                "PatternLayout": {
                    "pattern":"%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"
                }
            }
        },
        "loggers": {
            "root": {
                "level":"error",
                "appender-ref": {
                    "ref":"Console"
                }
            }
        }
    }
}
